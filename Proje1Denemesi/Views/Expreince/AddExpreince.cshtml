@model Expreince
@{
    ViewData["Title"] = "Deneyim Ekle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}


<form method="post">


    <div class="form-group">

        <label>Başlık</label>
        <input type="text" asp-for="Title" class="form-control" />
        <span class="text-danger" asp-validation-for="Title"></span>
    </div>

    <div class="form-group">

        <label>Başlama Yılı</label>
        <input type="number" asp-for="StartYear" class="form-control" />
        <span class="text-danger" asp-validation-for="StartYear"></span>

    </div>

    <div class="form-group">

        <label>Bitiş Yılı</label>
        <input type="number" asp-for="EndYear" class="form-control" />


    </div>

    <div class="form-group">

        <label>Şirket Adı</label>
        <input type="text" asp-for="Company" class="form-control" />
        <span class="text-danger" asp-validation-for="Company"></span>

    </div>
    <div class="form-group">

        <label>Şehir</label>
        <input type="text" asp-for="City" class="form-control" />
        <span class="text-danger" asp-validation-for="City"></span>

    </div>

    <div class="form-group">

        <label>Açıklama</label>
        <textarea type="text" asp-for="Description" class="form-control" rows="5"> </textarea>
        <span class="text-danger" asp-validation-for="Description"></span>




    </div>


    <button class="btn btn-outline-success">Kaydet</button>

</form>