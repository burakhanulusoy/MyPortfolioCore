@model Education
@{
    ViewData["Title"] = "Eğitim Hayatı Ekle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}





<form method="post">


    <div class="form-group">

        <label>Okul Adı</label>
        <input type="text" asp-for="SchoolName" class="form-control" />
        <span class="text-danger" asp-validation-for="SchoolName"></span>
    </div>

    <div class="form-group">

        <label>Başlama Yılı</label>
        <input type="number" asp-for="StartYear" class="form-control" />

    </div>

    <div class="form-group">

        <label>Bitiş Yılı</label>
        <input type="number" asp-for="EndYear" class="form-control" />
       

    </div>

    <div class="form-group">

        <label>Bölüm</label>
        <input type="text" asp-for="Department" class="form-control" />
        <span class="text-danger" asp-validation-for="Department"></span>

    </div>


    <div class="form-group">

        <label>Açıklama</label>
        <textarea type="text" asp-for="Description" class="form-control" rows="5"> </textarea>
        


    </div>
    
   
    <button class="btn btn-outline-success">Kaydet</button>





</form>
