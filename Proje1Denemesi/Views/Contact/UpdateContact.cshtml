
@model Contact

@{
    ViewData["Title"] = "İletişim Güncelleme";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}



<form method="post">

    <input type="hidden" asp-for="ContactId" />

    <div class="form-group">
        <label>Telefon Numarası</label>
        <input asp-for="Phone" class="form-control" type="text" />
        <span class="text-danger" asp-validation-for="Phone"></span>

    </div>

    <div class="form-group">
        <label>E-Mail Adresi</label>
        <input asp-for="Email" class="form-control" type="text" />
        <span class="text-danger" asp-validation-for="Email"></span>

    </div>

    <div class="form-group">
        <label>Adres</label>
        <input asp-for="Adress" class="form-control" type="text" />
        <span class="text-danger" asp-validation-for="Adress"></span>

    </div>


    <div class="form-group">
        <label>Harita Url'i'</label>
        <input asp-for="MapUrl" class="form-control" type="text" placeholder="google.maps'den alınız'" />
    </div>

    <button class="btn btn-outline-primary">Güncelle</button>



</form>

